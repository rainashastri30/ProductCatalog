	<a ng-href="http://localhost:8080/home/tvs/{{tvList[0]._id}}">Buy Now</a>
	
	<li>{{tvList[1].brand}}<br><br>
	<img src="{{tvList[1].img}}" height="500" width="600"/>
	</li><br><br>
	<a ng-href="http://localhost:8080/home/tvs/{{tvList[1]._id}}">Buy Now</a>
	
	<li>{{tvList[2].brand}}<br><br>
	<img src="{{tvList[2].img}}" height="500" width="600"/>
	</li><br><br>
	<a ng-href="http://localhost:8080/home/tvs/{{tvList[2]._id}}">Buy Now</a>
	
	<li>{{tvList[3].brand}}<br><br>
	<img src="{{tvList[3].img}}" height="500" width="600"/>
	</li><br><br>
	<a ng-href="http://localhost:8080/home/tvs/{{tvList[3]._id}}">Buy Now</a>
	<br><br>
	<a href="#/store/">Go Back to home</a><br><br>
	</div>
	</script>
	
	
	
	
	
	<script type="text/ng-template" id='/nailPolish.html'>
	<div id="section">
	<br><br>
	<B>Nail Polish</B><br><br>
	<li>{{npList[0].brand}}<br><br>
	<img src="{{npList[0].img}}" height="500" width="600"/>
	</li><br><br> 
	<a ng-href="http://localhost:8080/home/nps/{{npList[0]._id}}">Buy Now</a>
	
	<li>{{npList[1].brand}}<br><br>
	<img src="{{npList[1].img}}" height="500" width="600"/>
	</li><br><br> 
	<a ng-href="http://localhost:8080/home/nps/{{npList[1]._id}}">Buy Now</a>
	
	<li>{{npList[2].brand}}<br><br>
	<img src="{{npList[2].img}}" height="500" width="600"/>
	</li><br><br> 
	<a ng-href="http://localhost:8080/home/nps/{{npList[2]._id}}">Buy Now</a>
	
	<li>{{npList[3].brand}}<br><br>
	<img src="{{npList[3].img}}" height="500" width="600"/>
	</li><br><br> 
	<a ng-href="http://localhost:8080/home/nps/{{npList[3]._id}}">Buy Now</a>
	<br><br>
	<a href="#/store/">Go Back to home</a><br><br>
	</div>
	</script>
	
	
	
	
	
	
  	<div ng-view></div>
	
	
	<div id="footer"></div>
	
   
  </body>
</html>